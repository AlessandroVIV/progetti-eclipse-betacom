	public static void main(String[] args) {
		     
		System.out.println("Inizia MainPatternBuilder");

		Negozio negozio = new Negozio(12, "Mister Bean", "Via Appia 32, Roma", false);
		
		System.out.println("Negozio: " + negozio);
		
	}

}
