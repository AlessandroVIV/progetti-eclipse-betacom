package com.betacom.sb.component;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;
import com.betacom.sb.ProjectSbjdbcApplication;
import com.betacom.sb.dao.UfficiDAO;
import com.betacom.sb.models.Uffici;

@Component

public class BusinessComponent {

    private final ProjectSbjdbcApplication projectSbjdbcApplication;
	
	@Autowired
	private UfficiDAO uDao;

    BusinessComponent(ProjectSbjdbcApplication projectSbjdbcApplication) {
        this.projectSbjdbcApplication = projectSbjdbcApplication;
    }
	
	public void listUffici() {
		
		System.out.println("Inizia ListaUffici");
		
		List<Uffici> lU = uDao.getUffici();
		
		lU.forEach(u -> System.out.println(u));
		
	}
	
}
